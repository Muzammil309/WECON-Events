    "assets:upload": "node scripts/upload-to-supabase.js",
    "assets:test": "node scripts/setup-wecon-assets.js --test",
    "clone:setup": "npm run assets:download:complete && echo 'Pixel-perfect AIvent clone ready!'",
    "serve:static": "npx http-server . -p 8080 -o wecon-masawat-2025.html",
